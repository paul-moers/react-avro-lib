{
  "author": "Paul Moers",
  "description": "Avro Schema UI components",
  "devDependencies": {
    "@babel/core": "^7.18.13",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@cypress/react18": "^1.0.1",
    "@cypress/webpack-dev-server": "^2.0.0",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.10.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@storybook/addon-actions": "^6.5.10",
    "@storybook/addon-essentials": "^6.5.10",
    "@storybook/addon-links": "^6.5.10",
    "@storybook/react": "^6.5.10",
    "@swc/jest": "^0.2.22",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.6.5",
    "@types/react": "^18.0.17",
    "@types/react-cookies": "^0.1.0",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.25",
    "@types/styled-jsx": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-styled-components": "^2.0.7",
    "cypress": "^10.4.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.8.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "formik": "^2.2.9",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "ip": "^1.1.8",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jsdom": "^20.0.0",
    "msw": "^0.44.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.62.0",
    "rollup-plugin-dts": "^4.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.3.5",
    "tsconfig-paths-webpack-plugin": "^4.0.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-node-externals": "^3.0.0",
    "yup": "^1.0.0-beta.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "avro",
    "components",
    "react",
    "schema",
    "typescript",
    "ui"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": [
      "npm run pre-commit"
    ]
  },
  "main": "_bundles/main.js",
  "name": "react-avro-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/paul-moers/react-avro-lib"
  },
  "scripts": {
    "clean": "rm -rf _bundles lib lib-esm types dist",
    "bb": "npm run clean && tsc && tsc -m es6 --outDir lib-esm && webpack",

    "bt": "tsc --build tsconfig.json",

    "build": "webpack",
    "br": "rollup -c",
    "build-storybook": "build-storybook",
    "cypress": "cypress open",
    "cypress:ct": "cypress open --component",
    "cypress:ct:run": "cypress run --component --config video=false",
    "cypress:run": "cypress run --config video=false",
    "format": "prettier --write .",
    "husky-prepare": "husky install",
    "lint": "prettier --check . && eslint --ext .js,.jsx,.ts,.tsx --cache",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx --cache --fix .",
    "pre-commit": "npm run type-check && npm run format && npm run lint",
    "storybook": "start-storybook -p 6006",
    "test": "NODE_ENV=production tsc --build tests/jest/tsconfig.json && jest --env=node test",
    "test:coverage": "NODE_ENV=production tsc --build tests/jest/tsconfig.json && jest --env=node test --coverage",
    "type-check": "tsc --noEmit --emitDeclarationOnly false --declarationDir null"
  },
  "types": "./types/src/index.d.ts",
  "version": "0.1.9"
}
